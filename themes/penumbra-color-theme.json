  "tokenColors": [
    {
      "name": "variables",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "entity.name.variable",
        "meta.jsx.children",
        "variable.other.constant",
        "constant.other.color.rgb-value.hex",
        "text.html"
      ],
      "settings": {
        "foreground": "#b0a6ce"
      }
    },
    {
      "name": "properties",
      "scope": [
        "variable.other.property",
        "variable.object.property",
        "support.variable.property",
        "entity.other.attribute-name",
        "meta.object-literal.key",
        "support.type.property-name",
        "constant.numeric",
        "meta.definition.property",
        "variable.other.constant.property",
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#b292c7"
      }
    },
    {
      "name": "keyword / storage / punctuations",
      "scope": [
        "keyword",
        "storage",
        "punctuation.*",
        "punctuation.definition.tag",
        "punctuation.terminator",
        "punctuation.separator",
        "punctuation.accessor",
        "punctuation.separator.continuation",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.definition.entity",
        "punctuation.other.period",
        "punctuation.other.comma",
        "punctuation.other",
        "punctuation.section",
        "punctuation.support.type.property-name",
        "punctuation.definition.decorator",
        "punctuation.definition.annotation.java",
        "punctuation.definition.block.sequence.item.yaml",
        "markup.heading.markdown",
        "fenced_code.block.language.markdown"
      ],
      "settings": {
        "foreground": "#a08acf"
      }
    },
    {
      "name": "functions / methods / macros / preprocesso",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "entity.name.function.member",
        "meta.function",
        "support.function",
        "variable.language.super",
        "entity.name.function.preprocessor",
        "support.function.misc",
        "support.function.magic.python",
        "storage.type.annotation.java"
      ],
      "settings": {
        "foreground": "#968bc7"
      }
    },
    {
      "name": "types / enum members / interfaces",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.vendored.property-name",
        "support.constant.vendored.property-value",
        "meta.property-list",
        "keyword.other.unit",
        "variable.other.enummember",
        "entity.name.type.interface",
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#be8ba3"
      }
    },
    {
      "name": "parameters",
      "scope": [
        "variable.parameter",
        "constant.other.placeholder",
        "meta.property-name",
        "meta.at-rule.media.header",
        "constant.other.reference.link"
      ],
      "settings": {
        "foreground": "#b58cbe"
      }
    },
    {
      "name": "Primitive Types, Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.accessor",
        "keyword.operator.arithmetic",
