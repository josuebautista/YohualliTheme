      "settings": {
        "foreground": "#99b6a4"
      }
    },
    {
      "name": "numbers",
      "scope": ["constant.numeric", "constant.numeric.css"],
      "settings": {
        "foreground": "#ceaba1"
      }
    },
    {
      "name": "comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.css"
      ],
      "settings": {
        "foreground": "#969b97"
      }
    },
    {
      "name": "string interpolation",
      "scope": [
        "string.interpolated",
        "punctuation.definition.template-expression",
        "string",
        "support.constant.font-name",
        "string.quoted.raw.go",
        "string.interpolated.python",
        "constant.other.time",
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#9db4b2"
      }
    },
    {
      "name": "docstrings",
      "scope": [
        "comment.block",
        "comment.block.documentation",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#80a7af"
      }
    },
    {
      "name": "Markdown italics",
      "scope": ["markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
